{
  "name": "siopao",
  "version": "0.2.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "description": "A minimal HTTP routing library for Bun based on Radix Tree.",
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "bun run build-fast --dts-resolve",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun",
    "http",
    "fetch"
  ],
  "license": "MIT",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "dependencies": {
    "radix3": "^0.1.2"
  },
  "devDependencies": {
    "bun-types": "^0.1.2",
    "bumpp": "^8.2.1",
    "typescript": "^4.7.4",
    "tsup": "^6.1.3"
  }
}
